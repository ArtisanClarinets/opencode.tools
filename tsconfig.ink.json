{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2019",
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strict": false,
    "noImplicitAny": false
  },
  "include": [
    "src/**/*",
    "src/types/**/*",
    "types/**/*"
  ]
}
